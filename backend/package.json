{
  "name": "bhindi-backend",
  "version": "1.0.0",
  "description": "Backend API for Bhindi SaaS Clone",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "db:migrate": "node-pg-migrate up",
    "db:migrate:down": "node-pg-migrate down",
    "db:seed": "tsx src/database/seeds/index.ts",
    "worker": "tsx src/workers/index.ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "pg-pool": "^3.6.1",
    "redis": "^4.6.10",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "zod": "^3.22.4",
    "winston": "^3.11.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "stripe": "^14.10.0",
    "bull": "^4.12.0",
    "axios": "^1.6.2",
    "uuid": "^9.0.1",
    "date-fns": "^3.0.6",
    "openai": "^4.20.1",
    "@anthropic-ai/sdk": "^0.9.1",
    "socket.io": "^4.6.0",
    "multer": "^1.4.5-lts.1",
    "aws-sdk": "^2.1505.0",
    "nodemailer": "^6.9.7",
    "@sentry/node": "^7.91.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "@types/uuid": "^9.0.7",
    "@types/multer": "^1.4.11",
    "@types/nodemailer": "^6.4.14",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^6.0.2",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "node-pg-migrate": "^6.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
